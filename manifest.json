{
  "manifest_version": 3,
  "name": "GitHub Rulers",
  "version": "1.1",
  "description": "Displays 0, 80 & 120 character stop in the GitHub web editor",
  "icons": {
    "48": "48.png",
    "96": "96.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "*://*.github.com/*"
  ]
}
